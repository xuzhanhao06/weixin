.searchbar{
  background-color: #41be57;
  padding: 20rpx;
}
.search-navigator{
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;  
background-image:url("data:image/png;base64,");
background-position:2rpx 2rpx;
background-repeat: no-repeat;
background-size: 8%;

}
.search-input-group{
    /* width: 100%; */
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx; 
   padding: 10rpx 20rpx; /*上下10  左右20 */
  box-sizing: border-box;
  display: flex;
}
.search-input{
  min-height: 40rpx; 
  /* height: 40rpx;*/
  font-size: 32rpx;
  flex: 1;
}

.search-input-group button{
    display: flex;
    width: 120rpx;
    height: 100%;
    font-size: 32rpx;
    justify-content: center;
    align-items: center; 
     /* border-bottom: 0rpx solid #41be57; */
     background-color: #ccc;
     border-radius: 45rpx; 
}
